//descripcion este programa calcula el area de una circunferencia

#include "stdafx.h"
#include "stdio.h"
#include "conio.h"
void main()
{ 
  float area,radio;
printf("-----calculo del area de la circunferencia-----\n");
printf(" escriba el radio de la circunferencia\n");
scanf("%f",&radio);
area=(3.14159265359*(radio*radio));
 printf("\nel resultado es\n%f",area);
 printf("\n¶¶¶$¶¶¶¶¶¶¶¶¶¶¶¶¶øø¶¶¶¶¶¶¶¶¶$$¶¶¶\n¶¶¶$¶¶¶¶¶¶¶o´´´´´´´´´´´7¶¶¶¶¶¶$$¶\n$$¶¶¶¶$¶¶¶´´´111111111´´´´$¶¶¶¶$¶\n¶¶¶¶¶$¶¶ø´´11111111111111´´¶¶¶¶$¶\n¶¶¶¶¶¶¶ø´´1111111111111111´´´¶¶$¶\n¶¶¶¶¶¶¶´´1111111111111111171´¶¶¶¶\n¶¶$¶¶¶´´11111111111111111111´1¶¶$\n¶$¶¶$´111´´1´´´11´´´11111111´1¶¶$\n$¶¶¶1´77ø¶¶´ø¶¶¶ø¶¶¢11111111´7¶¶¶\n¶¶7´´´´´´´´¶´´´´´´´1¶7´11111´¶¶$¶\n¶¶´´¶´´´´´¶´´´´´´´´´´¶71111´´¶¶¶¶\n¶¶´´´´´´´´ø´´´ø¶´´´´´oo´´´´´¶¶$¶¶\n¶¶¶1ooøø71¶´´´´´´´´´´¶1´´¶7¢¶¶$¶¶\n¶¶¶´´1117´1¶´´´´´´´´$ø´´¶1¢¶¶¶$¶¶\n¶¶¶1´´´´´´´´7¶øoø¢¶¶7´´¶´´¶¶¶¶¶¶¶\n¶¶$1¶¶¶¶¶¶$¶ø´´´1´´´1171´$¶¶¶$$¶¶\n¶¶´1111111111¶¶¶7´´111´¶¢7¶¶¶$¶¶¶\n¶117777777777111ø¶1´11´1´¶´$¶$¶¶¶\n¶´7777777777777711¶´´11$´´´¶¶$¶¶¶\n$´17777777777777711¶´111o¶¶¶$¶¶¶¶\n¶7´´´1111111117777´¶´11´´¶$$¶¶¶¶¶\n¶¶¶¶¢øø¶¶¶¶ø¶¢7777´¶´111´¶$¶¶¶¶¶¶\n¶¶¶¶¶´´´11111o77711¶´111´¶¶¶¶¶¶¶¶\n¶¶¶¶¶¶¶´´17777111ø¶´´11´´¶¶¶¶¶¶¶¶\n¶¶¶¶$¶¶¶¶ø7117¢¶¶o´´´´´7¶¶´$¶¶¶¶¶\n¶¶¶$¶¶¢´o´ø¶$¢1´´1¶¶¶¶¶o´´´´¶¶$¶¶\n¶¶¶¶¶´o´ø´´´´ø¶7¶7´´´´´´´´´´7¶$¶¶\n¶(´´´´´ ´´´´´´´´´´´´´´´´´ ´´´¯)¶¶\n¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶¶");
